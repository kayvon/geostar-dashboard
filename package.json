{
  "name": "geostar-dashboard",
  "version": "0.0.1",
  "description": "dashboard for injested geostar data",
  "private": true,
  "type": "module",
  "scripts": {
    "build:client": "vite build --config vite.client.config.ts",
    "dev": "vite dev",
    "build": "npm run build:client && vite build",
    "deploy": "npm run build && wrangler deploy",
    "db:migrate:local": "wrangler d1 execute GEOSTAR_DB --local --persist-to ../.wrangler --file=../injest/schema.sql",
    "db:query": "wrangler d1 execute GEOSTAR_DB --local --persist-to ../.wrangler"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.25.2",
    "@types/node": "^25.3.0",
    "hono": "^4.12.0",
    "vite": "^7.3.1",
    "wrangler": "^4.60.0"
  }
}
